{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.11",
  "include": [
    "app",
    "main.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/alembic/versions"
  ],
  "reportMissingImports": "warning",
  "reportMissingModuleSource": "warning",
  "reportUndefinedVariable": "warning",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "pythonPlatform": "Linux"
}
